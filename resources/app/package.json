{
  "name": "ClipNuke",
  "version": "1.1.3",
  "description": "Weapon of Mass Distribution",
  "repository": "github:aidenvalentine/ClipNuke-Desktop-Win-x64",
  "homepage": "https://clipnuke.com",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "compile-win32-x64": "electron-packager . --platform=win32 --arch=x64",
    "publish": "build --win -p always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "build",
    "build": "electron-builder build --win --publish never"
  },
  "keywords": [
    "clips4sale",
    "manyvids",
    "xvideos",
    "pornhub",
    "clipnuke"
  ],
  "author": "ClipNuke.com",
  "license": "UNLICENSE",
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "20.38.0",
    "electron-packager": "^13.1.1",
    "electron-reload": "^1.4.0",
    "electron-windows-store": "^2.1.0",
    "spectron": "^5.0.0"
  },
  "dependencies": {
    "@woocommerce/woocommerce-rest-api": "^1.0.1",
    "any-file": "0.0.26",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "dateutil": "^0.1.0",
    "dotenv": "^8.0.0",
    "dropzone": "^5.5.1",
    "electron-log": "^3.0.8",
    "electron-navigation": "^1.5.8",
    "electron-preferences": "^1.3.2",
    "electron-settings": "^3.2.0",
    "electron-store": "^5.1.1",
    "electron-tabs": "^0.9.5",
    "electron-updater": "^4.0.4",
    "electron-winstaller": "^3.0.4",
    "express": "^4.16.4",
    "hashmap": "^2.3.0",
    "ip2proxy-nodejs": "^1.1.0",
    "ipc": "0.0.1",
    "jquery": "3.5.0",
    "path": "^0.12.7",
    "ping": "^0.2.2",
    "portscanner": "^2.2.0",
    "react": "^16.8.6",
    "selenium-standalone": "^6.16.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "test-port": "1.2.0",
    "url": "^0.11.0",
    "webdriverio": "^5.8.3",
    "woocommerce-api": "^1.4.2"
  },
  "build": {
    "appId": "com.github.aidenvalentine.clipnuke",
    "productName": "ClipNuke",
    "copyright": "Copyright Â© 2019 ClipNuke.com",
    "directories": {
      "buildResources": "../../build",
      "output": "../../dist",
      "app": "."
    },
    "extraResources": ["./resources/app/**", "./bin/**"],
    "mac": {
      "category": "your.app.category.type"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "aidenvalentine",
        "repo": "ClipNuke-Desktop-Win-x64",
        "protocol": "https",
        "private": true
      }
    ],
    "win": {
      "target": [
        "nsis",
        "zip",
        "msi"
      ],
      "publisherName": "ClipNuke",
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "artifactName": "clipnuke-${version} Setup.${ext}",
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  }
}
